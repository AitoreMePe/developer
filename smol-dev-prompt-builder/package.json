{
    "name": "smol-dev-prompt-builder",
    "displayName": "smol-dev-prompt-builder",
    "description": "An interactive interface for building prompts for smol_dev.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:smol-dev-prompt-builder.helloWorld",
        "onCommand:smol-dev-prompt-builder.openInterface"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "smol-dev-prompt-builder.helloWorld",
                "title": "Hello World (smol-dev-prompt-builder)"
            },
            {
                "command": "smol-dev-prompt-builder.openInterface",
                "title": "Open Smol Dev Prompt Builder Interface"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.80.0",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "20.2.5",
        "eslint": "^8.41.0",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^5.1.3",
        "ts-loader": "^9.4.3",
        "webpack": "^5.85.0",
        "webpack-cli": "^5.1.1",
        "@vscode/test-electron": "^2.3.2"
    }
}
